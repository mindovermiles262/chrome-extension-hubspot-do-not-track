{
  "name": "Hubspot Do Not Track",
  "version": "0.0.3",
  "description": "Sets do not track cookie for hubspot",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://villahomes.com/*"],
      "js": ["do-not-track.js"]
    }
  ],
  "icons": {
      "16": "images/villa.png",
      "32": "images/villa.png",
      "48": "images/villa.png",
      "128": "images/villa.png"
  },
  "manifest_version": 3
}
